{
  "name": "@pipeit/tx-orchestration",
  "version": "0.0.1-alpha",
  "description": "EXPERIMENTAL: Transaction orchestration for multi-step flows",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "experimental",
    "advanced",
    "solana",
    "transaction",
    "orchestration",
    "pipeline",
    "multi-step"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@solana/kit": "^5.0.0",
    "@solana/addresses": "*",
    "@solana/instructions": "*",
    "@solana/rpc": "*",
    "@solana/rpc-subscriptions": "*",
    "@solana/signers": "*"
  },
  "dependencies": {
    "@pipeit/tx-builder": "workspace:*"
  },
  "devDependencies": {
    "@solana/kit": "^5.0.0",
    "@types/node": "^24",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}

